            <object class="wxFlexGridSizer">
              <object class="sizeritem">
                <object class="wxCheckBox" name="ID_UPDATECHECK">
                  <label>&amp;Enable automatic update check</label>
                </object>
              </object>
              <object class="sizeritem">
                <object class="wxBoxSizer">
                  <orient>wxHORIZONTAL</orient>
