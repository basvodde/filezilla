<?xml version="1.0" encoding="UTF-8"?>
<resource version="2.3.0.1" xmlns="http://www.wxwindows.org/wxxrc">
  <object class="wxDialog" name="ID_SETTINGS">
    <title>Settings</title>
    <centered>1</centered>
    <object class="wxBoxSizer">
      <orient>wxHORIZONTAL</orient>
      <object class="sizeritem">
        <object class="wxBoxSizer">
          <orient>wxVERTICAL</orient>
          <object class="sizeritem">
            <object class="wxStaticText">
              <label>Select &amp;page:</label>
            </object>
            <flag>wxBOTTOM</flag>
            <border>5</border>
          </object>
          <object class="sizeritem">
            <object class="wxTreeCtrl" name="ID_TREE">
              <focused>1</focused>
              <style>wxSUNKEN_BORDER|wxTR_HAS_BUTTONS|wxTR_LINES_AT_ROOT|wxTR_HIDE_ROOT</style>
            </object>
            <option>1</option>
          </object>
          <object class="sizeritem">
            <object class="wxButton" name="wxID_OK">
              <default>1</default>
              <label>OK</label>
            </object>
            <flag>wxTOP|wxBOTTOM|wxGROW</flag>
            <border>5</border>
          </object>
          <object class="sizeritem">
            <object class="wxButton" name="wxID_CANCEL">
              <label>&amp;Cancel</label>
            </object>
            <flag>wxBOTTOM|wxGROW</flag>
            <border>2</border>
          </object>
        </object>
        <flag>wxALL|wxGROW</flag>
        <border>7</border>
      </object>
      <object class="sizeritem">
        <object class="wxBoxSizer">
          <orient>wxVERTICAL</orient>
          <object class="sizeritem">
            <object class="wxPanel" name="ID_PAGEPANEL"/>
            <option>1</option>
            <flag>wxGROW</flag>
          </object>
        </object>
        <option>1</option>
        <flag>wxTOP|wxBOTTOM|wxRIGHT|wxGROW</flag>
        <border>7</border>
      </object>
    </object>
  </object>
  <object class="wxPanel" name="ID_SETTINGS_CONNECTION">
    <object class="wxBoxSizer">
      <orient>wxVERTICAL</orient>
      <object class="sizeritem">
        <object class="wxStaticBoxSizer">
          <label>Overview</label>
          <orient>wxVERTICAL</orient>
          <object class="sizeritem">
            <object class="wxFlexGridSizer">
              <cols>1</cols>
              <object class="sizeritem">
                <object class="wxStaticText">
                  <label>For more detailed information about what these options do, please run the network configuration wizard.</label>
                </object>
              </object>
              <vgap>3</vgap>
              <object class="sizeritem">
                <object class="wxButton" name="ID_RUNWIZARD">
                  <label>&amp;Run configuration wizard now...</label>
                </object>
              </object>
            </object>
            <flag>wxBOTTOM|wxLEFT|wxRIGHT</flag>
            <border>4</border>
          </object>
        </object>
        <flag>wxGROW</flag>
      </object>
      <object class="sizeritem">
        <object class="wxStaticBoxSizer">
          <object class="sizeritem">
            <object class="wxFlexGridSizer">
              <cols>1</cols>
              <object class="sizeritem">
                <object class="wxFlexGridSizer">
                  <cols>3</cols>
                  <object class="sizeritem">
                    <object class="wxStaticText">
                      <label>Time&amp;out in seconds:</label>
                    </object>
                    <flag>wxALIGN_CENTRE_VERTICAL</flag>
                  </object>
                  <object class="sizeritem">
                    <object class="wxTextCtrl" name="ID_TIMEOUT">
                      <size>40,-1</size>
                    </object>
                    <flag>wxALIGN_CENTRE_VERTICAL</flag>
                  </object>
                  <hgap>4</hgap>
                  <object class="sizeritem">
                    <object class="wxStaticText">
                      <label>(5-9999, 0 to disable)</label>
                    </object>
                    <flag>wxALIGN_CENTRE_VERTICAL</flag>
                  </object>
                </object>
              </object>
              <object class="sizeritem">
                <object class="wxStaticText">
                  <label>If no data is sent or received during an operating for longer than the specified time, the connection will be closed and FileZilla will try to reconnect.</label>
                </object>
              </object>
              <vgap>4</vgap>
            </object>
            <flag>wxBOTTOM|wxLEFT|wxRIGHT</flag>
            <border>4</border>
          </object>
          <label>Timeout</label>
          <orient>wxVERTICAL</orient>
        </object>
        <flag>wxGROW</flag>
      </object>
      <object class="sizeritem">
        <object class="wxStaticBoxSizer">
          <object class="sizeritem">
            <object class="wxFlexGridSizer">
              <cols>3</cols>
              <object class="sizeritem">
                <object class="wxStaticText">
                  <label>&amp;Maximum number of retries:</label>
                </object>
                <flag>wxALIGN_CENTRE_VERTICAL</flag>
              </object>
              <object class="sizeritem">
                <object class="wxTextCtrl" name="ID_RETRIES">
                  <size>50,-1</size>
                </object>
                <flag>wxALIGN_CENTRE_VERTICAL</flag>
              </object>
              <object class="sizeritem">
                <object class="wxStaticText">
                  <label>(0-99)</label>
                </object>
                <flag>wxALIGN_CENTRE_VERTICAL</flag>
              </object>
              <object class="sizeritem">
                <object class="wxStaticText">
                  <label>&amp;Delay between failed login attempts:</label>
                </object>
                <flag>wxALIGN_CENTRE_VERTICAL</flag>
              </object>
              <object class="sizeritem">
                <object class="wxTextCtrl" name="ID_RETRYDELAY">
                  <size>50,-1</size>
                </object>
                <flag>wxALIGN_CENTRE_VERTICAL</flag>
              </object>
              <object class="sizeritem">
                <object class="wxStaticText">
                  <label>(0-999 seconds)</label>
                </object>
                <flag>wxALIGN_CENTRE_VERTICAL</flag>
              </object>
              <vgap>5</vgap>
              <hgap>5</hgap>
            </object>
            <flag>wxBOTTOM|wxLEFT|wxRIGHT</flag>
            <border>4</border>
          </object>
          <object class="sizeritem">
            <object class="wxStaticText">
              <label>Please note that some servers might ban you if you try to reconnect too often or in too short intervals.</label>
            </object>
            <flag>wxBOTTOM|wxLEFT|wxRIGHT</flag>
            <border>4</border>
          </object>
          <label>Reconnection settings</label>
          <orient>wxVERTICAL</orient>
        </object>
        <flag>wxGROW</flag>
      </object>
    </object>
  </object>
  <object class="wxPanel" name="ID_SETTINGS_CONNECTION_FTP">
    <object class="wxBoxSizer">
      <orient>wxVERTICAL</orient>
      <object class="sizeritem">
        <object class="wxStaticBoxSizer">
          <label>Transfer Mode</label>
          <orient>wxVERTICAL</orient>
          <object class="sizeritem">
            <object class="wxFlexGridSizer">
              <object class="sizeritem">
                <object class="wxRadioButton" name="ID_PASSIVE">
                  <label>Pa&amp;ssive (recommended)</label>
                  <style>wxRB_GROUP</style>
                </object>
              </object>
              <object class="sizeritem">
                <object class="wxRadioButton" name="ID_ACTIVE">
                  <label>&amp;Active</label>
                </object>
              </object>
              <object class="sizeritem">
                <object class="wxCheckBox" name="ID_FALLBACK">
                  <label>Allow &amp;fall back to other transfer mode on failure</label>
                </object>
              </object>
              <object class="sizeritem">
                <object class="wxStaticText">
                  <label>If you have problems to retrieve directory listings or to transfer files, try to change the default transfer mode.</label>
                </object>
              </object>
              <cols>1</cols>
              <vgap>5</vgap>
            </object>
            <flag>wxBOTTOM|wxLEFT|wxRIGHT</flag>
            <border>4</border>
          </object>
        </object>
        <flag>wxGROW</flag>
      </object>
      <object class="sizeritem">
        <object class="wxStaticBoxSizer">
          <label>FTP Keep-alive</label>
          <orient>wxVERTICAL</orient>
          <object class="sizeritem">
            <object class="wxGridSizer">
              <cols>1</cols>
              <vgap>5</vgap>
              <object class="sizeritem">
                <object class="wxCheckBox" name="ID_USEKEEPALIVE">
                  <label>Send FTP &amp;keep-alive commands</label>
                </object>
              </object>
              <object class="sizeritem">
                <object class="wxStaticText" name="">
                  <label>A proper server does not require this. Contact the server administrator if you need this.</label>
                </object>
                <flag>wxALIGN_TOP</flag>
              </object>
            </object>
            <flag>wxALL</flag>
            <border>4</border>
          </object>
        </object>
        <option>1</option>
        <flag>wxGROW</flag>
      </object>
    </object>
  </object>
  <object class="wxPanel" name="ID_SETTINGS_CONNECTION_ACTIVE">
    <object class="wxBoxSizer">
      <orient>wxVERTICAL</orient>
      <object class="sizeritem">
        <object class="wxStaticBoxSizer">
          <label>Limit local ports</label>
          <orient>wxVERTICAL</orient>
          <object class="sizeritem">
            <object class="wxCheckBox" name="ID_LIMITPORTS">
              <label>&amp;Limit local ports used by FileZilla</label>
            </object>
            <flag>wxALL</flag>
            <border>5</border>
          </object>
          <object class="sizeritem">
            <object class="wxBoxSizer">
              <orient>wxHORIZONTAL</orient>
              <object class="spacer">
                <size>18,0</size>
              </object>
              <object class="sizeritem">
                <object class="wxBoxSizer">
                  <orient>wxVERTICAL</orient>
                  <object class="sizeritem">
                    <object class="wxStaticText">
                      <label>By default uses any available local port to establish transfers in active mode. If you want to limit FileZilla to use only a small range of ports, please enter the port range below.</label>
                    </object>
                    <flag>wxBOTTOM</flag>
                    <border>5</border>
                  </object>
                  <object class="sizeritem">
                    <object class="wxFlexGridSizer">
                      <cols>2</cols>
                      <rows>2</rows>
                      <object class="sizeritem">
                        <object class="wxStaticText">
                          <label>Lo&amp;west available port:</label>
                        </object>
                        <flag>wxALIGN_CENTRE_VERTICAL</flag>
                      </object>
                      <object class="sizeritem">
                        <object class="wxTextCtrl" name="ID_LOWESTPORT">
                          <size>50,-1</size>
                        </object>
                      </object>
                      <object class="sizeritem">
                        <object class="wxStaticText">
                          <label>&amp;Highest available port:</label>
                        </object>
                        <flag>wxALIGN_CENTRE_VERTICAL</flag>
                      </object>
                      <object class="sizeritem">
                        <object class="wxTextCtrl" name="ID_HIGHESTPORT">
                          <size>50,-1</size>
                        </object>
                      </object>
                      <vgap>5</vgap>
                      <hgap>5</hgap>
                    </object>
                  </object>
                </object>
              </object>
            </object>
            <flag>wxBOTTOM|wxLEFT|wxRIGHT</flag>
            <border>5</border>
          </object>
        </object>
        <flag>wxGROW</flag>
      </object>
      <object class="sizeritem">
        <object class="wxStaticBoxSizer">
          <orient>wxVERTICAL</orient>
          <object class="sizeritem">
            <object class="wxFlexGridSizer">
              <object class="sizeritem">
                <object class="wxStaticText">
                  <label>In order to use active mode, FileZilla needs to know your external IP address.</label>
                </object>
              </object>
              <object class="sizeritem">
                <object class="wxRadioButton" name="ID_ACTIVEMODE1">
                  <label>&amp;Ask your operating system for the external IP address</label>
                  <style>wxRB_GROUP</style>
                </object>
              </object>
              <object class="sizeritem">
                <object class="wxRadioButton" name="ID_ACTIVEMODE2">
                  <label>&amp;Use the following IP address:</label>
                </object>
              </object>
              <object class="sizeritem">
                <object class="wxBoxSizer">
                  <orient>wxVERTICAL</orient>
                  <object class="sizeritem">
                    <object class="wxTextCtrl" name="ID_ACTIVEIP"/>
                  </object>
                  <object class="sizeritem">
                    <object class="wxStaticText">
                      <label>Use this if you're behind a router and have a static external IP address.</label>
                    </object>
                  </object>
                </object>
                <flag>wxLEFT</flag>
                <border>17</border>
              </object>
              <object class="sizeritem">
                <object class="wxRadioButton" name="ID_ACTIVEMODE3">
                  <label>&amp;Get external IP address from the following URL:</label>
                </object>
              </object>
              <object class="sizeritem">
                <object class="wxBoxSizer">
                  <orient>wxVERTICAL</orient>
                  <object class="sizeritem">
                    <object class="wxTextCtrl" name="ID_ACTIVERESOLVER"/>
                    <flag>wxBOTTOM|wxGROW</flag>
                    <border>3</border>
                  </object>
                  <object class="sizeritem">
                    <object class="wxStaticText">
                      <label>Default: http://ip.filezilla-project.org/ip.php</label>
                    </object>
                  </object>
                </object>
                <flag>wxLEFT</flag>
                <border>17</border>
              </object>
              <object class="sizeritem">
                <object class="wxCheckBox" name="ID_NOEXTERNALONLOCAL">
                  <label>&amp;Don't use external IP address on local connections.</label>
                </object>
              </object>
              <cols>1</cols>
              <vgap>3</vgap>
            </object>
            <flag>wxBOTTOM|wxLEFT|wxRIGHT</flag>
            <border>4</border>
          </object>
          <label>Active mode IP</label>
        </object>
        <flag>wxGROW</flag>
      </object>
    </object>
  </object>
  <object class="wxPanel" name="ID_SETTINGS_CONNECTION_PASSIVE">
    <object class="wxBoxSizer">
      <orient>wxVERTICAL</orient>
      <object class="sizeritem">
        <object class="wxStaticBoxSizer">
          <label>Passive mode</label>
          <orient>wxVERTICAL</orient>
          <object class="sizeritem">
            <object class="wxFlexGridSizer">
              <object class="sizeritem">
                <object class="wxStaticText">
                  <label>Some misconfigured remote servers which are behind a router, may reply with their local IP address.</label>
                </object>
              </object>
              <object class="sizeritem">
                <object class="wxRadioButton" name="ID_PASSIVE_FALLBACK1">
                  <label>&amp;Use the server's external IP address instead</label>
                  <style>wxRB_GROUP</style>
                </object>
              </object>
              <object class="sizeritem">
                <object class="wxRadioButton" name="ID_PASSIVE_FALLBACK2">
                  <label>&amp;Fall back to active mode</label>
                </object>
              </object>
              <cols>1</cols>
              <vgap>3</vgap>
              <rows>2</rows>
            </object>
            <flag>wxLEFT|wxRIGHT|wxBOTTOM</flag>
            <border>4</border>
          </object>
        </object>
        <flag>wxGROW</flag>
      </object>
    </object>
  </object>
  <object class="wxPanel" name="ID_SETTINGS_CONNECTION_FTP_PROXY">
    <object class="wxBoxSizer">
      <orient>wxVERTICAL</orient>
      <object class="sizeritem">
        <object class="wxStaticBoxSizer">
          <label>FTP Proxy</label>
          <orient>wxHORIZONTAL</orient>
          <object class="sizeritem">
            <object class="wxFlexGridSizer">
              <cols>1</cols>
              <vgap>5</vgap>
              <object class="sizeritem">
                <object class="wxStaticText">
                  <label>Type of FTP Proxy:</label>
                </object>
              </object>
              <object class="sizeritem">
                <object class="wxRadioButton" name="ID_PROXYTYPE_NONE">
                  <label>&amp;None</label>
                </object>
              </object>
              <object class="sizeritem">
                <object class="wxRadioButton" name="ID_PROXYTYPE_USER">
                  <label>USER@&amp;HOST</label>
                </object>
              </object>
              <object class="sizeritem">
                <object class="wxRadioButton" name="ID_PROXYTYPE_SITE">
                  <label>&amp;SITE</label>
                </object>
              </object>
              <object class="sizeritem">
                <object class="wxRadioButton" name="ID_PROXYTYPE_OPEN">
                  <label>&amp;OPEN</label>
                </object>
              </object>
              <object class="sizeritem">
                <object class="wxRadioButton" name="ID_PROXYTYPE_CUSTOM">
                  <label>Cus&amp;tom</label>
                </object>
              </object>
              <object class="sizeritem">
                <object class="wxBoxSizer">
                  <orient>wxVERTICAL</orient>
                  <object class="sizeritem">
                    <object class="wxTextCtrl" name="ID_LOGINSEQUENCE">
                      <style>wxTE_MULTILINE</style>
                    </object>
                    <option>1</option>
                    <flag>wxGROW</flag>
                  </object>
                  <object class="sizeritem">
                    <object class="wxStaticText">
                      <label>Format specifications:</label>
                    </object>
                  </object>
                  <object class="sizeritem">
                    <object class="wxBoxSizer">
                      <orient>wxHORIZONTAL</orient>
                      <object class="sizeritem">
                        <object class="wxStaticText">
                          <label>%h - Host</label>
                        </object>
                      </object>
                      <object class="spacer">
                        <size>20,0</size>
                      </object>
                      <object class="sizeritem">
                        <object class="wxStaticText">
                          <label>%u - Username</label>
                        </object>
                      </object>
                      <object class="spacer">
                        <size>20,0</size>
                      </object>
                      <object class="sizeritem">
                        <object class="wxStaticText">
                          <label>%p - Password</label>
                        </object>
                      </object>
                    </object>
                  </object>
                  <object class="sizeritem">
                    <object class="wxStaticText">
                      <label>%a - Account (Lines containing this will be omitted if not using Account logontype)</label>
                    </object>
                  </object>
                  <object class="sizeritem">
                    <object class="wxBoxSizer">
                      <orient>wxHORIZONTAL</orient>
                      <object class="sizeritem">
                        <object class="wxStaticText">
                          <label>%s - Proxy user</label>
                        </object>
                      </object>
                      <object class="spacer">
                        <size>20,0</size>
                      </object>
                      <object class="sizeritem">
                        <object class="wxStaticText">
                          <label>%w - Proxy password</label>
                        </object>
                      </object>
                    </object>
                  </object>
                </object>
                <option>1</option>
                <flag>wxLEFT|wxGROW</flag>
                <border>17</border>
              </object>
              <object class="sizeritem">
                <object class="wxFlexGridSizer">
                  <cols>5</cols>
                  <object class="sizeritem">
                    <object class="wxStaticText">
                      <label>P&amp;roxy host:</label>
                    </object>
                    <flag>wxALIGN_CENTRE_VERTICAL</flag>
                  </object>
                  <object class="sizeritem">
                    <object class="wxTextCtrl" name="ID_PROXY_HOST"/>
                    <flag>wxALIGN_CENTRE_VERTICAL</flag>
                  </object>
                  <object class="spacer">
                    <size>10,0</size>
                  </object>
                  <object class="sizeritem">
                    <object class="wxStaticText">
                      <label>Proxy &amp;user:</label>
                    </object>
                    <flag>wxALIGN_CENTRE_VERTICAL</flag>
                  </object>
                  <object class="sizeritem">
                    <object class="wxTextCtrl" name="ID_PROXY_USER"/>
                    <flag>wxALIGN_CENTRE_VERTICAL</flag>
                  </object>
                  <object class="sizeritem">
                    <object class="wxStaticText">
                      <label>Pro&amp;xy password:</label>
                    </object>
                    <flag>wxALIGN_CENTRE_VERTICAL</flag>
                  </object>
                  <object class="sizeritem">
                    <object class="wxTextCtrl" name="ID_PROXY_PASS">
                      <style>wxTE_PASSWORD</style>
                    </object>
                    <flag>wxALIGN_CENTRE_VERTICAL</flag>
                  </object>
                  <vgap>5</vgap>
                  <hgap>5</hgap>
                </object>
              </object>
              <growablecols>0</growablecols>
              <object class="sizeritem">
                <object class="wxStaticText">
                  <label>Note: This only works with plain, unencrypted FTP connections.</label>
                </object>
              </object>
              <growablerows>6</growablerows>
            </object>
            <option>1</option>
            <flag>wxBOTTOM|wxLEFT|wxRIGHT|wxGROW</flag>
            <border>5</border>
          </object>
        </object>
        <option>1</option>
        <flag>wxGROW</flag>
      </object>
    </object>
  </object>
  <object class="wxPanel" name="ID_SETTINGS_CONNECTION_SFTP">
    <object class="wxFlexGridSizer">
      <cols>2</cols>
      <vgap/>
      <object class="sizeritem">
        <object class="wxStaticBoxSizer">
          <label>Public Key Authentication</label>
          <orient>wxVERTICAL</orient>
          <object class="sizeritem">
            <object class="wxFlexGridSizer">
              <cols>1</cols>
              <object class="sizeritem">
                <object class="wxStaticText" name="">
                  <label>To support public key authentication, FileZilla needs to know the private keys to use.</label>
                </object>
              </object>
              <object class="spacer"/>
              <object class="sizeritem">
                <object class="wxStaticText" name="">
                  <label>Private &amp;keys:</label>
                </object>
              </object>
              <object class="sizeritem">
                <object class="wxListCtrl" name="ID_KEYS">
                  <style>wxLC_REPORT|wxSUNKEN_BORDER</style>
                </object>
                <option>1</option>
                <flag>wxGROW</flag>
              </object>
              <vgap>5</vgap>
              <growablecols>0</growablecols>
              <object class="sizeritem">
                <object class="wxGridSizer">
                  <object class="sizeritem">
                    <object class="wxButton" name="ID_ADDKEY">
                      <label>&amp;Add keyfile...</label>
                    </object>
                  </object>
                  <object class="sizeritem">
                    <object class="wxButton" name="ID_REMOVEKEY">
                      <label>&amp;Remove key</label>
                    </object>
                  </object>
                  <cols>2</cols>
                  <hgap>5</hgap>
                </object>
                <flag>wxALIGN_CENTRE_HORIZONTAL</flag>
              </object>
              <object class="sizeritem" platform="win">
                <object class="wxStaticText">
                  <label>Alternatively you can use the Pageant tool from PuTTY to manage your keys, FileZilla does recognize Pageant.</label>
                </object>
              </object>
              <object class="sizeritem" platform="mac|unix">
                <object class="wxStaticText">
                  <label>Alternatively you can use your system's SSH agent. To do so, make sure the SSH__AUTH__SOCK environment variable is set.</label>
                </object>
              </object>
              <growablerows>3</growablerows>
            </object>
            <option>1</option>
            <flag>wxALL|wxGROW</flag>
            <border>5</border>
          </object>
        </object>
        <option>1</option>
        <flag>wxGROW</flag>
      </object>
      <growablecols>0</growablecols>
      <growablerows>0</growablerows>
    </object>
  </object>
  <object class="wxPanel" name="ID_SETTINGS_CONNECTION_PROXY">
    <object class="wxBoxSizer">
      <orient>wxVERTICAL</orient>
      <object class="sizeritem">
        <object class="wxStaticBoxSizer">
          <label>Generic proxy</label>
          <orient>wxHORIZONTAL</orient>
          <object class="sizeritem">
            <object class="wxFlexGridSizer">
              <cols>1</cols>
              <vgap>5</vgap>
              <object class="sizeritem">
                <object class="wxStaticText">
                  <label>Type of generic proxy:</label>
                </object>
              </object>
              <object class="sizeritem">
                <object class="wxRadioButton" name="ID_PROXYTYPE_NONE">
                  <label>&amp;None</label>
                </object>
              </object>
              <object class="sizeritem">
                <object class="wxRadioButton" name="ID_PROXYTYPE_HTTP">
                  <label>&amp;HTTP/1.1 using CONNECT method</label>
                </object>
              </object>
              <object class="sizeritem">
                <object class="wxRadioButton" name="ID_PROXYTYPE_SOCKS5">
                  <label>&amp;SOCKS 5</label>
                </object>
              </object>
              <object class="sizeritem">
                <object class="wxFlexGridSizer">
